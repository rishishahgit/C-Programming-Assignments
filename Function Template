#include <stdio.h>

// Function declaration
int add(int a, int b);

// Main function
int main() {
    int num1, num2, result;

    printf("Enter two numbers to add:\n");
    scanf("%d %d", &num1, &num2);

    // Function call
    result = add(num1, num2);

    printf("The sum of %d and %d is %d\n", num1, num2, result);

    return 0;
}

// Function definition
int add(int a, int b) {
    return a + b;
}
